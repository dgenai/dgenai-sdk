{
  "name": "dgenai-sdk",
  "version": "1.0.4",
  "description": "Official TypeScript SDK for dgenai Public API",
  "license": "MIT",
  "type": "module",
  "bin": {
    "dgenai": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "x402-axios": "^0.6.6"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.6.2"
  }
}
